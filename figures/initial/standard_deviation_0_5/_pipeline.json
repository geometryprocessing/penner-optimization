{
  "global_args": {
    "fname": [
      "bumpy_torus.obj"
    ],
    "input_dir": "data/closed-Myles",
    "energy_choice": "surface_hencky_strain",
    "colormap": "scale_factors",
    "histogram_choice": "scale_factors",
    "direction_choice": "unconstrained_minimizer",
    "use_optimal_projection": true,
    "initial_pert_sd": 0.5,
    "min_ratio": 1e-6,
    "height": 1600,
    "width": 2560
  },
  "pipeline":
  [
    {
      "method": "optimize",
      "skip": false,
      "args_list": [
        {}
      ]
    },
    {
      "method": "vf_pipeline",
      "skip": false,
      "args_list": [
        {
          "suffix": "conf"
        },
        {
          "suffix": "opt"
        }
      ]
    },
    {
      "method": "histogram",
      "skip": false,
      "args_list": [
        {
          "suffix": "conf",
          "color": "red",
          "bin_min": -5,
          "bin_max": 5
        },
        {
          "suffix": "opt",
          "color": "blue",
          "bin_min": -1.0,
          "bin_max": 1.0
        }
      ]
    },
    {
      "method": "render_pipeline",
      "skip": false,
      "args_list": [
        {
          "suffix": "conf_refined_with_uv"
        },
        {
          "suffix": "opt_refined_with_uv"
        }
      ]
    }
  ]
}

